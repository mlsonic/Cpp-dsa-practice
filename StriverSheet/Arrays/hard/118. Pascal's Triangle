//Brute Force TC(N*N) SC(N*N)
class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        int n= numRows;
        vector<vector<int>> v;
        for(int i=1;i<=n;i++){
            vector<int> a(i,1);
            if(n>=3){
                for(int j=1;j<a.size()-1;j++){
                        a[j]=v[i-2][j-1]+v[i-2][j];
                        cout << a[j];
                }
            }
            v.push_back(a);
        }
        return v;
    }
};
