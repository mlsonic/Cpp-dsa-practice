class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        vector<int> ans;
        ans.reserve(2*nums.size());// vector ko dyanmically bar bar memory reallocate nah karna padege isliye reserve use karte hai
    for(int i=0;i<2;i++){
        for(int x:nums) ans.push_back(x);
    }
        return ans;
    }
};


class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans(2 * n);
        for(int i=0;i<2*n;i++){
            ans[i]=nums[i%n];//0,1,2,3%3=0,1,2
        }
        return ans;
    }
};

class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans = nums;
        ans.insert(ans.end(),ans.begin(),ans.end());
        return ans;
    }
};

class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        vector<int> ans;
        ans.reserve(2*nums.size());

        for(int x:nums) ans.push_back(x);
        for(int x:nums) ans.push_back(x);

        return ans;
    }
};
