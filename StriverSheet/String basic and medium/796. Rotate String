//My brute force approuch
class Solution {
public:
    bool rotateString(string s, string goal) {
        int n=s.size();
        string a=s;
        for(int i=0;i<=n;i++){
            reverse(s.begin(),s.begin()+i);
            reverse(s.begin()+i,s.end());
            reverse(s.begin(),s.end());
            if(s==goal) return true;
            s=a;
        }
        return false;
        
    }
};
