class Solution {
public:
    string removeOuterParentheses(string s) {
        string a;
        int level=0;
        for(char ch:s){
            if(ch=='('){
                if(level>0) a+=ch; // skip outer most as in starting it is zero
                level++;
            }
            else if(ch==')'){
                level--;
                if(level>0) a+=ch; skip outer most 
            }

        }
        return a;
    }
};
