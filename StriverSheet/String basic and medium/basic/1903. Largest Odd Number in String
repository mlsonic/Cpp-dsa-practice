class Solution {
public:
    string largestOddNumber(string num){
        for(int i=num.size()-1;i>=0;i--){
            //here we will check from back if its odd as we are staring from last i will be largest
            if((num[i]-'0')%2==1) { // convert into integer only valid for 0-9 ascii
                return num.substr(0,i+1);//substr cut string till we wan we can also use resize or iterate till i+1 as last one is not included.
            }
        }
        return "";
    }
};
