
int print(Node* head){
    while(head!=NULL){
    cout<<head->data<<" ";
    head=head->next;
    }
}
// for removing head
Node* removeHead(Node* head){
    if(head==NULL) return head;
    Node* temp=head;
    head=head->next;
    delete temp;//delete karna isliye jaruri hai because woh store raheta hai heap memory toh memory leakage hota faltu mai ram use hote raheti
    return head;
}

//removing tail
Node* removetail(Node* head){
    if(head==NULL || head->next == NULL ){
        if(head->next == NULL) delete head;
        return NULL;
    }
    Node* temp = head;
    while(temp->next->next !=NULL){
        temp=temp->next;
    }
    delete temp->next;
    temp->next=nullptr;
    return head;

}
//removing the kth element
Node* removek(Node* head,int k){
    if(head==NULL) return head;
    if(k==1){
        Node* temp=head;
        head=head->next;
        delete temp;
        return head;
    }
    int cnt=0;
    Node* temp=head;
    Node* prev=NULL;
    while(temp!=NULL){
        cnt++;
        if(cnt==k){
            prev->next=prev->next->next;
            break;
        }
        prev = temp;
        temp=temp->next;
    }
    return head;
}

