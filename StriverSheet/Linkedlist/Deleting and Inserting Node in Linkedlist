
int print(Node* head){
    while(head!=NULL){
    cout<<head->data<<" ";
    head=head->next;
    }
}
// for removing head
Node* removeHead(Node* head){
    if(head==NULL) return head;
    Node* temp=head;
    head=head->next;
    delete temp;//delete karna isliye jaruri hai because woh store raheta hai heap memory toh memory leakage hota faltu mai ram use hote raheti
    return head;
}

//removing tail
Node* removetail(Node* head){
    if(head==NULL || head->next == NULL ){
        if(head->next == NULL) delete head;
        return NULL;
    }
    Node* temp = head;
    while(temp->next->next !=NULL){
        temp=temp->next;
    }
    delete temp->next;
    temp->next=nullptr;
    return head;

}

int main(){
    vector<int>  arr ={5,6,8,9};
    Node* head = convertarrtoll(arr);
    head = removeHead(head);
    print(head);
}




