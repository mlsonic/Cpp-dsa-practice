
int print(Node* head){
    while(head!=NULL){
    cout<<head->data<<" ";
    head=head->next;
    }
}

Node* removeHead(Node* head){
    if(head==NULL) return head;
    Node* temp=head;
    head=head->next;
    delete temp;//delete karna isliye jaruri hai because woh store raheta hai heap memory toh memory leakage hota faltu mai ram use hote raheti
    return head;
}

int main(){
    vector<int>  arr ={5,6,8,9};
    Node* head = convertarrtoll(arr);
    head = removeHead(head);
    print(head);
}

