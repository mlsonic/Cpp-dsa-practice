// brute force Tc=O(n*2*log n) sc = O(n)
class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        map<ListNode*, int> mpp;
        ListNode* temp = head;
        while(temp){
            if(mpp.find(temp) != mpp.end()) return temp; //agar repeat hua temp hi answer hai
            mpp[temp] = 1;
            temp = temp->next; 
        }
        return nullptr;
    }
};
