//Better arproch tc= O(log(n))
class Solution {
public:
    const long long MOD = 1e9 + 7;
    long long power(long long a, long long b) {
    if (b == 0) return 1;
    long long half = power(a, b / 2);
    if (b % 2 == 0)
        return (half * half) % MOD;
    else
        return (half * half * a) % MOD;
    }

    int countGoodNumbers(long long n) {
        long long even = (n + 1) / 2;
        long long odd  = n / 2;
        long long ans = (power(5, even) * power(4, odd)) % MOD;
        return (int)ans;
    }
};


//brute force 
class Solution {
public:
    int countGoodNumbers(long long n) {
        const long long MOD = 1e9 + 7;
        long long ans = 1;
        while (n > 0) {
            if (n % 2 == 1) {
                ans = (ans * 5) % MOD;
            } else {
                ans = (ans * 4) % MOD;
            }
            n--;
        }
        return (int)ans;
    }
};
