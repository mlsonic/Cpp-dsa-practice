// Using recursion tc = O(pow(2,n))  sc = O(n)
class Solution {
public:
    void sq(int i, vector<vector<int>> &ans, vector<int> &a ,vector<int> nums){
        if(i>=nums.size()){
            ans.push_back(a);
            return;
        }
        a.push_back(nums[i]);
        sq(i+1,ans,a , nums);
        a.pop_back();
        sq(i+1,ans,a , nums);
    }

    vector<vector<int>> subsets(vector<int>& nums) {
        vector<vector<int>> ans;
        vector<int> a;
        int i=0;
        sq(i,ans,a , nums);
        return ans;
    }
};
