class Solution {
  public:
  
    bool func(int i, int n, vector<int>& arr, int k){
        if (i >= n) return k == 0;
        if (k < 0 ) return false;
        if(func(i+1, n, arr, k-arr[i]) == true ) return true;
        if(func(i+1, n, arr, k) == true ) return true;
        return false;
    }
    
    bool checkSubsequenceSum(int n, vector<int>& arr, int k) {
        return func(0, n, arr, k);
    }
};
