//striver code
class MyQueue {
    stack<int> st;
    stack<int> s;
public:
    MyQueue() {
        
    }
    
    void push(int x) {
        while(!st.empty()){
            s.push(st.top());
            st.pop();
        }
        st.push(x);
        while(!s.empty()){
            st.push(s.top());
            s.pop();
        }  
    }
    
    int pop() {
        int a = st.top();
        st.pop();
        return a;
    }
    
    int peek() {
       return st.top();
    }
    
    bool empty() {
        return st.empty();
    }
};

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue* obj = new MyQueue();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->peek();
 * bool param_4 = obj->empty();
 */

//My Brute force approch
class MyQueue {
    stack<int> st;
public:
    MyQueue() {
        
    }
    
    void push(int x) {
        st.push(x);
    }
    
    int pop() {
        stack<int> s;
        while(!st.empty()){
            s.push(st.top());
            st.pop();
        }
        int a = s.top();
        s.pop();
        while(!s.empty()){
            st.push(s.top());
            s.pop();
        }
        return a;
    }
    
    int peek() {
        stack<int> s;
        while(!st.empty()){
            s.push(st.top());
            st.pop();
        }
        int a = s.top();
        while(!s.empty()){
            st.push(s.top());
            s.pop();
        }
        return a;
    }
    
    bool empty() {
        return st.empty();
    }
};

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue* obj = new MyQueue();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->peek();
 * bool param_4 = obj->empty();
 */
