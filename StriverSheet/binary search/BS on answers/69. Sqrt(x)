class Solution {
public:
    int mySqrt(int x) {
        int low=0,high=x;
        while(low<=high){
            int mid=(low+high)/2;
            if(1LL*mid*mid<=x) {
                low=mid+1;
                }
            else high=mid-1;
        }
        return high;//as while get false high move at position where last possible answer can be store so it is answer
    }
};

//By binary search 
class Solution {
public:
    int mySqrt(int x) {
        int low=0,high=x,ans=0;
        while(low<=high){
            int mid=(low+high)/2;
            if(1LL*mid*mid<=x) {// 1LL is used to stop overflow as value can be huge
                low=mid+1;
                ans=mid;
                }
            else high=mid-1;
        }
        return ans;
    }
};
//brute force tC=O(N)
class Solution {
public:
    int mySqrt(int x) {
        int ans=0;
        for(int i=0;i<=x;i++){
            if(1LL*i*i<=x){ //1LL is to stop overflow as i*i exced limits
                ans=i;
            }
            else break;
        }
        return ans;
    }
};
