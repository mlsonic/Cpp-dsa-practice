class Solution {
public:
    int search(vector<int>& nums, int target) {
        int n=nums.size();
        int low=0,high=n-1;
        while(low<=high){
            int mid= (low+high)/2;
            if(target==nums[mid]) return mid;
            else if(target>nums[mid]) low=mid+1;
            else high=mid-1;
        }
        return -1;
    }
};
// time complexity  is O(log2(n))
// can be done by recursion
// for overflow case take mid as low +(high-low)/2
